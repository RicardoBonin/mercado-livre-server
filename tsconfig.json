{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,

    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "src/models/User.ts"],
  "exclude": ["node_modules"]
}
